<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:columnCount="4"
        android:rowCount="5"
        android:columnOrderPreserved="false">

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_previous" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton2"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_play" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton3"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_stop" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton4"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_next" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton5"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_mute" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton6"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_volume_down" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton7"
            android:layout_row="1"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_volume_up" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton8"
            android:layout_row="1"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_loop" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton9"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="All"
            android:textSize="14dp"
            android:src="@drawable/all"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton10"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="R&amp;B"
            android:textSize="14dp"
            android:src="@drawable/rb"
            android:scaleType="fitCenter" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton11"
            android:layout_row="2"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="Rock"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/rock" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton12"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="Classic"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/classic" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton13"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="Soundtracks"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/soundtrack" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton14"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="90&apos;"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/s90" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton15"
            android:layout_row="3"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="80&apos;"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/s80" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton16"
            android:layout_row="3"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="70"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/s70" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton25"
            android:layout_row="4"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:src="@drawable/music_player_music_player_icon"
            android:scaleType="fitCenter" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton28"
            android:layout_row="4"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="Maroon5"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/maroon5" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton27"
            android:layout_row="4"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="Ceiline Dion"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/cd" />

        <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton26"
            android:layout_row="4"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="@drawable/button_study3"
            android:text="Carpenter"
            android:textSize="14dp"
            android:scaleType="fitCenter"
            android:src="@drawable/carpenters" />

    </GridLayout>
</RelativeLayout>